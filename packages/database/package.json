{
  "name": "@subfy/database",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./src/index.ts",
  "exports": {
    ".": {
      "types": "./src/index.ts",
      "require": "./dist/index.cjs",
      "import": "./dist/index.js"
    }
  },
  "scripts": {
    "dev": "tsup --watch",
    "build": "tsup",
    "typecheck": "tsc -p tsconfig.json --noEmit",
    "lint": "eslint .",
    "clean": "rimraf dist"
  },
  "dependencies": {
    "@nestjs/common": "^11.0.1",
    "@subfy/firebase": "workspace:*",
    "firebase-admin": "^12.7.0"
  },
  "devDependencies": {
    "tsup": "^8.2.4",
    "typescript": "^5.7.3"
  }
}
