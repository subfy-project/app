{
  "name": "subfy",
  "private": true,
  "packageManager": "pnpm@10.26.1",
  "engines": {
    "node": ">=20"
  },
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "scripts": {
    "dev": "turbo dev",
    "build": "turbo build",
    "typecheck": "turbo typecheck",
    "lint": "turbo lint",
    "test": "turbo test",
    "clean": "turbo clean && rimraf .turbo node_modules"
  },
  "devDependencies": {
    "rimraf": "^6.0.1",
    "turbo": "^2.5.6"
  },
  "pnpm": {
    "onlyBuiltDependencies": ["@nestjs/core", "sharp", "unrs-resolver"]
  }
}
